{
    "name": "Blockchain Adblock",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "",
    "icons": {
        "512": "images/logo512.png"
    },
    "browser_action": {
        "default_icon": "images/logo512.png",
        "default_popup": "index.html",
        "default_title": "Blockchain Adblock"
    },
    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
